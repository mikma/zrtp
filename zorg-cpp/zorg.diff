+Zorg_Cache * Zorg_Impl_Cache_Create(Zorg_Error * e, const char * file, Zorg_CryptoSuite * cryptoSuite)
+{
+    return ZRTP::Impl::CreateCache(*e, file, cryptoSuite);
+}
