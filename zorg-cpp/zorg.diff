+PJ_DEF(pj_status_t) pjmedia_transport_zrtp_go_clear(pjmedia_transport *tp)
+{
+    transport_zrtp *zrtp = (transport_zrtp*) tp;
+
+    PJ_ASSERT_RETURN(zrtp, PJ_EINVAL);
+
+    /* TBD: this is not supported by zrtp.org yet */
+    return PJ_ENOTSUP;
+}
